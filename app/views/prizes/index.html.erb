<div class="scaf_table">
<h3>当選者一覧</h3>
<div class="scaf_button">
  <%= link_to '新規登録', new_prize_path %>
</div>
<table>
  <thead>
    <tr>
      <th>学籍番号</th>
      <th>景品名</th>
      <th>登録者</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @prizes.each do |prize| %>
      <tr>
        <td><%= link_to prize.student_no, prize %></td>
        <td><%= prize.prize %></td>
        <td><%= prize.reg_user %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>